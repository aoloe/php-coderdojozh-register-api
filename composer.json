{
    "name": "aoloe/coderdojozh-registration-api",
    "description": "PHP API for the CoderDojo Zurich Registration Management",
    "keywords": [],
    "homepage": "http://github.com/aoloe/js-coderdojozh-registration-api",
    "license": "MIT",
    "authors": [
        {
            "name": "ale rimoldi",
            "email": "ale@graphicslab.org",
            "homepage": "http://ideale.ch"
        }
    ],
    "repositories" : [
        {
            "type": "vcs",
            "url":  "https://github.com/aoloe/php-meetup"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "aoloe/meetup": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "Aoloe\\CoderDojoZh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public public/index.php",
        "test": "phpunit"
    }

}
